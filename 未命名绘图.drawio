        <mxCell id="AAbSFF_3w6VQspWgnhy2-25" value="一主两从三哨兵" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;strokeColor=none;fillColor=none;" parent="1" vertex="1">
